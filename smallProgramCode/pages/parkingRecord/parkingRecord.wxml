<navigation-bar title="车位申请记录" back="{{false}}" color="white" background="#59c0df"></navigation-bar>
<view style="height:50rpx;width:50rpx;margin-top:-72rpx;" bindtap="back">
	<image src="/images/return.png" style="height:50rpx;width:50rpx;margin-left: 20rpx;"></image>
</view>
<scroll-view scroll-y="true" type="list" style="margin-top: 40rpx;height: 1100rpx;width: 100%;">
	<view style="display: flex;flex-direction: column;">
		<view wx:for="{{parkingAList}}" style="margin:20rpx auto" wx:key="item">
			<parkingABlock parkingAItem="{{item}}"></parkingABlock>
			<view style="margin-top:-60rpx;margin-left: 30rpx;" wx:if="{{item.auditResult==='通过'&&item.time}}">
				<text style="color: red;" wx:if="{{!(item.time.min==0&&item.time.second==0)}}">倒计时：{{item.time.min}}分{{item.time.second}}秒，请尽快支付申请车位费</text>

			</view>
			<view wx:if="{{!item.time&&item.auditResult==='已失效'}}" style="margin-top:-60rpx;margin-left: 30rpx;">
				<text style="color: red;">计时结束，申请已失效</text>
			</view>
		</view>

	</view>

</scroll-view>